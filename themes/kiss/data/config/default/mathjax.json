{
  "library": {
    "path": "//cdn.jsdelivr.net/npm/mathjax@3/es5/tex-mml-chtml.js",
    "config": "TeX-MML-AM_CHTML"
  },
  "config": {
    "tex2jax": {
      "inlineMath": [["$", "$"], ["\\(", "\\)"]],
      "processEscapes": true
    },
    "TeX": {
      "equationNumbers": { 
        "autoNumber": "AMS"
      },
      "extensions": ["AMSmath.js"]
    }
  }
}
